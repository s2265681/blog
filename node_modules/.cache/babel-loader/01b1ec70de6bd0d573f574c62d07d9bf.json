fs[formName].resetFields();\n      },\n\n    }\n};\n</script>\n<style>\n.title {\n  position: fixed;\n  width: 100%;\n  text-align: center;\n  font-size: 0.6rem;\n  color: #5d2f2f;\n  top: 0.25rem;\n}\n\n.header {\n  background-color: #55547be0;\n  width:'100%'\n}\n.login {\n  background-color: #55547be0;\n  height: 100vh;\n}\n .loginForm .el-form-item__label{\n        color: #333;\n    }\n</style>"],"sourceRoot":"src/pages"}]}