\">\n            <el-input type=\"password\" v-model.number=\"loginForm.password\" auto-complete=\"off\"></el-input>\n            </el-col>\n            </el-form-item>\n            <el-form-item>\n              <el-button type=\"primary\" @click=\"submitForm('loginForm')\"  size=\"mini\">提交</el-button>\n              <el-button @click=\"resetForm('loginForm')\"  size=\"mini\">重置</el-button>\n            </el-form-item>\n</el-form>\n    </div>\n\n <!-- <alert-tip v-if=\"true\" :showHide=\"showAlert\" :alertText=\"1111\"></alert-tip> -->\n\n  </div>\n</template>\n<script>\nimport { accountLogin } from './../service/getData'\nexport default {\n  name: \"login\",\n  data(){\n    return{\n          loginForm: {\n             username: '',\n             password:'',\n\n           \n        },\n          showAlert: false,\n          userInfo:''\n    }\n    \n  },\n  methods: {\n       //发送登录信息\n            async submitForm(){\n                    if (!this.loginForm.username) {\n                        return\n                    }else if(!this.loginForm.password){\n                        return\n                    }\n                    //用户名登录\n                    this.userInfo = await accountLogin(this.loginForm.username, this.loginForm.password);\n                    if(this.userInfo.errno===-1){\n                       this.$message({\n                            message:this.userInfo.message,\n                            type:'error'\n                          });\n                    }else{\n                          this.$message({\n                            message:this.userInfo.message,\n                            type:'success'\n                          });\n                          this.$router.push('/admin')\n                    }\n\n              // await getBlogList()\n              await accountLogin(this.loginForm.username,this.loginForm.password)\n            },\n      resetForm(formName) {\n        this.$refs[formName].resetFields();\n      },\n\n    }\n};\n</script>\n<style>\n.title {\n  position: fixed;\n  width: 100%;\n  text-align: center;\n  font-size: 0.6rem;\n  color: #5d2f2f;\n  top: 0.25rem;\n}\n\n.header {\n  background-color: #55547be0;\n  width:'100%'\n}\n.login {\n  background-color: #55547be0;\n  height: 100vh;\n}\n .loginForm .el-form-item__label{\n        color: #333;\n    }\n</style>"]}]}